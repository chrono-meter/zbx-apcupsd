                <item>
                    <uuid>8fad9bbf9d6843079b0faa752225d54b</uuid>
                    <name>UPS Battery Replacement Date</name>
                    <type>DEPENDENT</type>
                    <key>apcupsd.BATTDATE</key>
                    <delay>0</delay>
                    <history>7d</history>
                    <trends>0</trends>
                    <units>unixtime</units>
                    <preprocessing>
                        <step>
                            <type>JSONPATH</type>
                            <parameters>
                                <parameter>$.BATTDATE</parameter>
                            </parameters>
                        </step>
                        <step>
                            <type>JAVASCRIPT</type>
                            <parameters>
                                <parameter>return new Date(value).getTime() / 1000;</parameter>
                            </parameters>
                        </step>
                    </preprocessing>
                    <master_item>
                        <key>apcupsd.apcaccess</key>
                    </master_item>
                    <tags>
                        <tag>
                            <tag>Application</tag>
                            <value>UPS</value>
                        </tag>
                    </tags>
                    <triggers>
                        <trigger>
                            <uuid>f7dbc88416be4441819a32e11fa1d3f2</uuid>
                            <expression>last(/APCUPSd/apcupsd.BATTDATE) &lt; now() - (3 * 365 * 24 * 60 * 60)</expression>
                            <name>3 years gone after battery replacement</name>
                            <priority>WARNING</priority>
                        </trigger>
                        <trigger>
                            <uuid>53c9549567fe4c459589474e589e6026</uuid>
                            <expression>last(/APCUPSd/apcupsd.BATTDATE) &lt; now() - (4 * 365 * 24 * 60 * 60)</expression>
                            <name>4 years gone after battery replacement</name>
                            <priority>AVERAGE</priority>
                        </trigger>
                        <trigger>
                            <uuid>f6711e1466ba4a9ca4392a8ba807efab</uuid>
                            <expression>last(/APCUPSd/apcupsd.BATTDATE) &lt; now() - (5 * 365 * 24 * 60 * 60)</expression>
                            <name>5 years gone after battery replacement</name>
                            <priority>HIGH</priority>
                        </trigger>
                    </triggers>
                </item>
