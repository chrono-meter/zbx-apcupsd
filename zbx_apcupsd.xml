                        </tag>
                    </tags>
                    <triggers>
                        <trigger>
                            <uuid>b58f4b7c5dc941a4bc7e3b07fa57c30d</uuid>
                            <expression>find(/APCUPSd/apcupsd.Status,#1,&quot;regexp&quot;,&quot;^(ONLINE)$&quot;)=0</expression>
                            <name>UPS Power Interruption</name>
                            <priority>AVERAGE</priority>
                            <dependencies>
                                <dependency>
                                    <name>UPS Recharging</name>
                                    <expression>find(/APCUPSd/apcupsd.Status,1s,&quot;regexp&quot;,&quot;^(CHARGING)$&quot;)=1</expression>
                                </dependency>
                                <dependency>
                                    <name>UPS Time Remaining Very Low</name>
                                    <expression>last(/APCUPSd/apcupsd.TIMELEFT)&lt;{$APC_REM_CRIT}</expression>
                                </dependency>
                            </dependencies>
                        </trigger>
                        <trigger>
                            <uuid>5da72f304a434131a6fa5e899d69b401</uuid>
                            <expression>find(/APCUPSd/apcupsd.Status,1s,&quot;regexp&quot;,&quot;^(CHARGING)$&quot;)=1</expression>
                            <name>UPS Recharging</name>
                            <priority>INFO</priority>
                            <manual_close>YES</manual_close>
                        </trigger>
                    </triggers>
